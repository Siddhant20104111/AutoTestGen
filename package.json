{
  "name": "hello-world-svelte",
  "displayName": "hello-world-svelte",
  "description": "A hello world sample extension to demonstrate using Svelte with the Webview UI Toolkit for VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "myextension-sidebar-view",
          "title": "Unit Test",
          "icon": "media/icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World (Svelte): Show"
      }
    ],
    "views": {
      "myextension-sidebar-view": [
        {
          "type": "webview",
          "id": "myextension-sidebar",
          "name": "UnitTest",
          "icon": "media/icon.svg",
          "contextualTitle": "MyExtension"
        },
        {
          "type": "webview",
          "id": "myextension-sidebar2",
          "name": "UnitTest2",
          "icon": "media/icon.svg",
          "contextualTitle": "MyExtension2"
        },
        {
          "id": "myextension-treeview",
          "name": "Second Panel"
        }
      ]
    }
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run dev",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "rebuild": "electron-rebuild -f -w tree-sitter -v 18.18.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "tree-sitter-cli": "^0.20.8",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@types/mustache": "^4.2.5",
    "clarifai-nodejs-grpc": "^10.1.1",
    "maven": "^5.0.0",
    "mustache": "^4.2.0",
    "tree-sitter": "^0.20.6",
    "tree-sitter-java": "^0.20.2",
    "web-tree-sitter": "^0.20.8"
  }
}
